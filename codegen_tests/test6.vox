fun fibonnacci(x)
{
	print x;
	if x < 2 {
		return x;
	}
	return fibonnacci(x - 1) + fibonnacci(x - 2);
}

fun fibo_with_var(x)
{
	var a = 2;
	if x < a {
		var a = 0;
		return x;
	}
	return fibo_with_var(x - 1) + fibo_with_var(x - 2);
}

