var list = [0,2,3,1,1,7,6,9];
var r = 0;

fun sort(l, n)
{
	var count = 0;
	var i = 0;
	var tmp = 0;

	if n == 1 {
		return 0;
	}

	for (i = 0; i < n - 1; i = i + 1) {
		if list[i] > list[i + 1] {
			tmp = list[i];
			list[i] = list[i + 1];
			list[i + 1] = tmp;
			count = count + 1;
		}
	}

	if count == 0 {
		return 0;
	}

	tmp = sort(l, n - 1);
}

