fun logical_exp_test(arg1, arg2)
{
	if (arg1) // Error
		return 0;
	if (#arg1)
		return 1;
	else if (#arg2[0])
		return 1;
	else if (!#arg1)
		return 2;
	else if (!#arg2[2])
		return 1;
	else if (true)
		return #arg2[0];

	return !arg1[0]; // Error
	return !#arg1[0];
}

